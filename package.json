    "generate": "stencil generate",
    "docker": "./docker.sh start"
  },
  "dependencies": {
    "@stencil/core": "^2.11.0",
    "@stencil/sass": "^1.3.2",
    "koa": "^2.13.4",
    "koa-static": "^5.0.0",
    "moment": "^2.29.1",
    "nyanyajs-log": "^1.0.10"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/react-output-target": "0.0.9",
    "typescript": "^4.5.2"
  }
